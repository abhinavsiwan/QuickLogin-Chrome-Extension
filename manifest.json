{
    "manifest_version": 2,
    
    "name": "Qucik Login",
    "description": "Login into multiple accounts with one click",
    "version": "1",
    
    "browser_action": {
    "default_icon": "itc1.png",
	"default_title": "QuickLogin",
	"default_popup": "popup.html"
    },
    "icons": {
        "64": "itc1.png",
        "32": "itc1.png",
        "16": "itc1.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "content_scripts": [
        {
            "matches": ["http://*/*","https://*/*"],
            "js": ["jquery.js","myScript.js"]
        }
    ],
    "permissions": [
        "notifications",
        "https://*.facebook.com/*","http://*.facebook.com/*",
        "tabs",
        "activeTab",
        "storage",
        "background",
        "management"
    ]
}